HashKnownHosts no
CanonicalizeHostname yes
CanonicalDomains magi.blue magi.vpn magi.lan statusim.net hosts.dap.ps
CanonicalizeMaxDots 4
CanonicalizeFallbackLocal yes

Host *
    ServerAliveInterval=5
    ServerAliveCountMax=60
    ForwardAgent yes
    SetEnv TERM=xterm

Host *.magi.blue *.magi.vpn *.magi.lan
    User jakubgs

Host *.statusim.net *.hosts.dap.ps
    User admin
    VerifyHostKeyDNS yes

Host 127.0.0.1
    UserKnownHostsFile /dev/null

Host ubuntuvm
    HostName 192.168.122.202
